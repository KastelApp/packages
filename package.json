{
	"name": "@kastelll/kastel",
	"private": true,
	"version": "0.0.0",
	"description": "Kastel's core packages",
	"scripts": {
		"build": "turbo run build",
		"build:affected": "turbo run build --filter='...[origin/master]'",
		"test": "turbo run test --parallel",
		"test:affected": "turbo run test --filter='...[origin/master]' --parallel",
		"lint": "turbo run lint --parallel",
		"lint:affected": "turbo run lint --filter='...[origin/master]' --parallel",
		"format": "turbo run format --parallel",
		"format:affected": "turbo run format --filter='...[origin/master]' --parallel",
		"fmt": "turbo run format --parallel",
		"fmt:affected": "turbo run format --filter='...[origin/master]' --parallel",
		"docs": "turbo run docs --parallel",
		"docs:affected": "turbo run docs --filter='...[origin/master]' --parallel",
		"postinstall": "is-ci || husky install",
		"update": "yarn upgrade-interactive",
		"create-package": "node packages/scripts/src/packageScript.mjs",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-angular": "^17.6.1",
		"husky": "^8.0.0",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.2.1",
		"node-gyp": "^9.3.1",
		"tsup": "^6.7.0",
		"turbo": "latest",
		"typescript": "^5.0.4",
		"vitest": "^0.29.8"
	},
	"license": "ISC",
	"repository": {
		"url": "https://github.com/kastelll/packages.git",
		"type": "git"
	},
	"contributors": [
		{
			"email": "darkerinker@gmail.com",
			"name": "DarkerInk"
		}
	],
	"engines": {
		"node": ">=16.9.0"
	},
	"workspaces": [
		"packages/*"
	],
	"packageManager": "yarn@3.5.0"
}
